{
    "name": "burtds/cash-converter",
    "description": "A simple cash converter package for Laravel.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Burtds\\CashConverter\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Bert De Swaef",
            "email": "bert@bert.gent"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.9",
        "spatie/laravel-package-tools": "^1.16",
        "php": ">=8.0"
    },
    "extra": {
        "laravel": {
          "providers": [
            "Burtds\\CashConverter\\Providers\\CashConverterServiceProvider"
          ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "orchestra/testbench": "^8.13",
        "spatie/laravel-ray": "^1.33"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
